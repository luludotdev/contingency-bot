{
  "name": "contingency-bot",
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "main": "./dist/index.js",
  "repository": "git@github.com:lolPants/contingency-bot.git",
  "author": "Jack Baron <jackmbaron@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "prettier --check . && eslint src --ext ts",
    "format": "prettier --write . && eslint src --ext ts --fix",
    "fmt": "yarn run format",
    "check": "yarn run tsc --noEmit --skipLibCheck",
    "test": "yarn run check && yarn run lint",
    "build": "tsup",
    "dev": "yarn run build --watch",
    "debug": "nodemon --watch build --inspect"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "@lolpants/eslint-config": "^0.4.0",
    "@swc/core": "^1.3.46",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.15.11",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@lolpants/env": "^0.6.3",
    "@lolpants/exit": "^0.2.0",
    "@lolpants/jogger": "^0.8.1",
    "discord-api-types": "^0.37.37",
    "discord.js": "^14.9.0",
    "discordx": "^11.7.4",
    "execa": "^7.1.1",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "tslib": "^2.5.0"
  }
}
