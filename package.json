{
  "name": "contingency-bot",
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "main": "./build/index.js",
  "repository": "git@github.com:lolPants/contingency-bot.git",
  "author": "Jack Baron <jackmbaron@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16.9.0"
  },
  "scripts": {
    "lint": "prettier --check . && eslint src --ext mjs,js,ts",
    "format": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "fmt": "yarn run format",
    "clean": "rimraf build",
    "compile": "ttsc",
    "check": "yarn run compile --noEmit",
    "build": "yarn run clean && yarn run compile",
    "test": "yarn run check && yarn run lint",
    "dev": "yarn run clean && yarn run compile --watch",
    "debug": "nodemon --watch build --inspect"
  },
  "devDependencies": {
    "@lolpants/eslint-config": "^0.1.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.8.3",
    "eslint": "^8.25.0",
    "eslint-config-neon": "^0.1.35",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/utilities": "^5.1.0",
    "@lolpants/env": "^0.6.1",
    "@lolpants/exit": "^0.2.0",
    "@lolpants/jogger": "^0.8.1",
    "discord-api-types": "^0.37.12",
    "discord.js": "^14.5.0",
    "discordx": "^11.4.0",
    "execa": "^6.1.0",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.0"
  }
}
