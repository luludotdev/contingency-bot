{
  "name": "contingency-bot",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "main": "./build/index.js",
  "repository": "git@github.com:lolPants/contingency-bot.git",
  "author": "Jack Baron <jackmbaron@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16.6.0"
  },
  "scripts": {
    "test": "yarn run check && yarn run lint",
    "check": "yarn run compile --noEmit",
    "lint": "xo ./src",
    "compile": "ttsc",
    "clean": "rimraf build",
    "build": "yarn run clean && yarn run compile",
    "dev": "yarn run clean && yarn run compile --watch",
    "debug": "nodemon --watch build --inspect"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/node": "^18.6.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1",
    "xo": "^0.51.0"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/utilities": "^5.0.0",
    "@lolpants/env": "^0.5.3",
    "@lolpants/exit": "^0.1.0",
    "@lolpants/jogger": "^0.7.1",
    "discord-api-types": "^0.37.0",
    "discord.js": "^14.1.2",
    "discordx": "^11.0.4",
    "execa": "^6.1.0",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.0"
  }
}
